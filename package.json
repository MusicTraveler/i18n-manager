{
  "name": "i18n-manager",
  "description": "A comprehensive i18n string and locale management tool built with Next.js, for editing and maintaining translation messages.",
  "private": true,
  "scripts": {
    "build": "next build",
    "check": "npm run build && tsc",
    "db:generate-types": "kysely-codegen",
