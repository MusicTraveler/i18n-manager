    "@tanstack/react-virtual": "^3.13.12",
    "@trpc/client": "^11.7.0",
    "@trpc/server": "^11.7.0",
    "date-fns": "^4.1.0",
    "kysely": "^0.27.0",
    "kysely-codegen": "^0.19.0",
    "next": "16.0.0",
    "normalize.css": "^8.0.1",
    "pg": "^8.16.3",
